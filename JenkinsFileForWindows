pipeline{
	agent any
	stages {
		stage('Docker Build'){
		    steps{
		        dir('c:/project/RosalShop/'){
		        bat 'docker build -t ngabroger/websiteproject:latest .'
		        }
		    }
		    
		}
		stage('Docker Login'){
		    steps{
		        dir('c:/project/RosalShop/'){
		        bat 'docker login -u ngabroger -p dckr_pat_nMQHnsf1vaamiaDcojGVAKaSbZw'
		        }
		    }
		}
        stage('Docker Push'){
	    steps{
	        dir('c:/project/RosalShop/'){
	        bat 'docker push ngabroger/websiteproject:latest'
	        }
	    }
	   }
	}
	
}
